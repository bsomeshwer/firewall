{
  "name": "someshwer/firewall",
  "description": "This package allows us to protect our website from un authorized users by providing access to only whitelisted ip addresses or by blocking ip addresses by blacklist.",
  "type": "composer-plugin",
  "keywords": [
    "firewall",
    "block",
    "blacklist",
    "whitelist"
  ],
  "homepage": "https://github.com/bsomeshwer/firewall",
  "license": "MIT",
  "authors": [
    {
      "name": "Someshwer Bandapally",
      "email": "bsomeshwer89@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "support": {
    "issues": "https://github.com/bsomeshwer/firewall/issues",
    "source": "https://github.com/bsomeshwer/firewall"
  },
  "require": {
    "php": "^7.1 || ^7.2 || >=7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0 || ^8.0 || ^9.0"
  },
  "autoload": {
    "psr-4": {
      "Someshwer\\Firewall\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Someshwer\\Firewall\\Tests\\": "src/tests"
    }
  }
}
